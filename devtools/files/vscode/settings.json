{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "colorize.colorized_variables": [
    "CSS"
  ],
  "colorize.languages": [
    "javascript",
    "typescript",
    "typescriptreact"
  ],
  "debug.console.fontFamily": "CozetteVector",
  "debug.openDebug": "neverOpen",
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.volumes.sortBy": "Label",
  "editor.accessibilitySupport": "off",
  "editor.codeLensFontFamily": "CozetteVector",
  "editor.fontFamily": "CozetteVector",
  "editor.fontSize": 12,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.inlayHints.fontFamily": "CozetteVector",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    80,
    120
  ],
  "editor.snippetSuggestions": "bottom",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.wordBasedSuggestionsMode": "currentDocument",
  "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "off",
  "emojisense.languages": {
    "git-commit": true,
    "markdown": true,
    "plaintext": {
      "emojiDecoratorsEnabled": false,
      "markupCompletionsEnabled": false
    },
    "scminput": true,
    "typescript": {
      "emojiDecoratorsEnabled": false,
      "markupCompletionsEnabled": false
    },
    "typescriptreact": {
      "emojiDecoratorsEnabled": false,
      "markupCompletionsEnabled": false
    }
  },
  "errorLens.fontFamily": "CozetteVector",
  "eslint.alwaysShowStatus": true,
  "eslint.lintTask.enable": true,
  "eslint.lintTask.options": "src/",
  "eslint.run": "onSave",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "**/*_vars/**/*.yml": "ansible",
    "**/*ansible*/**/*.yml": "ansible",
    "**/defaults/**/*": "ansible",
    "**/handler/*.yml": "ansible",
    "**/inventory/*/*": "ansible",
    "**/playbooks/**/*.yml": "ansible",
    "**/roles/**/*.yml": "ansible",
    "**/tasks/**/*.yml": "ansible",
    "**/vars/**/*.yml": "ansible",
    "*.fastfile": "ruby",
    "*.go": "go",
    "*.plist": "xml",
    "*.tsx": "typescriptreact",
    "*.yml": "yaml",
    "docker-compose*.yml": "dockercompose",
    "docker-compose.yml": "dockercompose"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/coverage/**": true,
    "**/log/**": true,
    "**/node_modules/*/**": true,
    "**/tmp/**": true
  },
  "git-graph.commitDetailsView.location": "Docked to Bottom",
  "git.allowNoVerifyCommit": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.autofetch": "all",
  "git.confirmNoVerifyCommit": false,
  "git.confirmSync": false,
  "git.fetchOnPull": true,
  "git.mergeEditor": true,
  "git.suggestSmartCommit": false,
  "github-actions.org-features": true,
  "github.gitProtocol": "ssh",
  "githubIssues.ignoreCompletionTrigger": [
    "diff",
    "git-commit",
    "git-rebase",
    "ignore",
    "julia",
    "r"
  ],
  "githubIssues.ignoreUserCompletionTrigger": [],
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    },
    {
      "label": "Recent Issues",
      "query": "state:open repo:${owner}/${repository} sort:updated-desc"
    }
  ],
  "githubIssues.workingIssueFormatScm": "fix|feat: ${issueTitle} \nFixes ${issueNumberLabel}",
  "githubPullRequests.assignCreated": "${user}",
  "githubPullRequests.pushBranch": "always",
  "githubPullRequests.remotes": [
    "origin",
    "upstream",
    "github"
  ],
  "gitlens.advanced.abbreviateShaOnCopy": true,
  "gitlens.advanced.abbreviatedShaLength": 8,
  "gitlens.advanced.fileHistoryShowAllBranches": true,
  "gitlens.advanced.messages": {
    "suppressCreatePullRequestPrompt": true
  },
  "gitlens.blame.heatmap.location": "left",
  "gitlens.blame.ignoreWhitespace": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.scopes": [
    "blocks"
  ],
  "gitlens.defaultGravatarsStyle": "mp",
  "gitlens.hovers.enabled": false,
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.views.branches.files.layout": "tree",
  "gitlens.views.formats.commits.label": "${message}",
  "gitlens.views.repositories.branches.showBranchComparison": false,
  "gitlens.views.repositories.includeWorkingTree": true,
  "gitlens.views.repositories.showContributors": false,
  "go.enableCodeLens": {
    "references": true
  },
  "go.toolsManagement.autoUpdate": true,
  "javascript-repl.context": "Node",
  "javascript.referencesCodeLens.enabled": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "jest.autoRun": "off",
  "jest.enableSnapshotPreviews": false,
  "jest.enableSnapshotUpdateMessages": false,
  "jest.shell": "/bin/zsh",
  "liveshare.alwaysFollowHost": true,
  "liveshare.audio.startCallOnShare": true,
  "liveshare.presence": true,
  "liveshare.publishWorkspaceInfo": true,
  "markdown-everywhere.preview-mode": "fenced",
  "markdown.experimental.validate.fileLinks.markdownFragmentLinks": "warning",
  "markdown.extension.list.indentationSize": "inherit",
  "markdown.extension.print.imgToBase64": true,
  "markdown.extension.print.includeVscodeStylesheets": false,
  "markdown.extension.syntax.plainTheme": true,
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "markdown.extension.toc.levels": "2..6",
  "markdown.preview.openMarkdownLinks": "inEditor",
  "markdown.styles": [
    "https://gist.githubusercontent.com/JamesMessinger/5d31c053d0b1d52389eb2723f7550907/raw/41ac88d0cd00c55bac925891296df05c894c4a34/github-markdown.css"
  ],
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "merge-conflict.diffViewPosition": "Beside",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "handydandy-notebook": "left",
    "jupyter-notebook": "left"
  },
  "notebook.outputFontFamily": "CozetteVector",
  "powershell.buttons.showPanelMovementButtons": true,
  "powershell.integratedConsole.focusConsoleOnExecute": false,
  "redhat.telemetry.enabled": false,
  "remote.SSH.connectTimeout": 60,
  "remote.SSH.useFlock": false,
  "scm.alwaysShowActions": true,
  "scm.inputFontFamily": "CozetteVector",
  "security.workspace.trust.untrustedFiles": "newWindow",
  "settingsSync.ignoredExtensions": [
    "dendron.dendron"
  ],
  "sshfs.configs": [],
  "sync.gist": "0a65b2ddd3960752068f57ee0fe62f54",
  "terminal.integrated.allowChords": false,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontFamily": "CozetteVector",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "args": [
        "-l"
      ],
      "path": "bash"
    },
    "zsh": {
      "args": [
        "-l"
      ],
      "path": "zsh"
    }
  },
  "terminal.integrated.profiles.osx": {
    "bash": {
      "args": [
        "-l"
      ],
      "path": "bash"
    },
    "zsh": {
      "args": [
        "-l"
      ],
      "path": "zsh"
    }
  },
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "path": "pwsh.exe"
    }
  },
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.tabs.enabled": true,
  "terminal.external.windowsExec": "alacritty",
  "terminal.external.linuxExec": "alacritty",
  "terminal.external.osxExec": "alacritty",
  "testing.autoRun.mode": "rerun",
  "testing.openTesting": "neverOpen",
  "typescript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsCodeGitWorktrees.move.openNewVscodeWindow": true,
  "vsCodeGitWorktrees.remove.stalledBranches": true,
  "window.commandCenter": true,
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editorAssociations": {
    "*.gz": "default",
    "*.ipynb": "jupyter-notebook",
    "git-rebase-todo": "gitlens.rebase"
  },
  "workbench.enableExperiments": false,
  "workbench.layoutControl.enabled": false,
  "workbench.tree.indent": 4,
  "yaml.schemas": {}
}
